function set_style_from_cookie() {
    var style_theme = localStorage.getItem('style_theme');
    var theme_value = localStorage.getItem('theme_value');

    if (!style_theme && !theme_value) {
        style_theme = 'blue';
        theme_value = 'light';
    }
    $('#themesbox').val(theme_value);
    switch_style(style_theme);
    localStorage.setItem('theme_value', theme_value);
}

$("#themesbox").on('change', function() {
    var theme = $('#themesbox option:selected').val();
    switch_style(theme);
    localStorage.setItem('theme_value', theme);
})

