//change handler
var incdec = function (dir, change) {
    let offSet = parseInt(localStorage.getItem('font_offSet')) || 0
    if (offSet + change <= 2 && offSet + change >= 0) {
        offSet += change
        localStorage.setItem('font_offSet', offSet)
        items.forEach(function (item) {
            size(change, item)
        })
    }
}

var size = function (change, item) {
    var el = document.querySelector(item.el)
    var style = window.getComputedStyle(el, null).getPropertyValue('font-size')
    var currentFontSize = parseFloat(style)
    el.style.fontSize = currentFontSize + change + 'px'

}

