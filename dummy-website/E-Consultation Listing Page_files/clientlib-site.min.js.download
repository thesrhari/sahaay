/*--------------making the switch connection for all english pages to hindi pages indivudually-------------*/
$(document).ready(function() {
    var strVal = encrypt(window.location.href);
    $("#languagebox").on("change", function() {
        var ktmVal = $("#languagebox").val();
        if (ktmVal == "english") {
            var resVal = decrypt(decodeURIComponent(strVal));
            resVal = resVal.replace("/hi","/en");
            window.location.href = resVal;
        } else {
            var resVal = decrypt(decodeURIComponent(strVal));
            resVal = resVal.replace("/en", "/hi");
            window.location.href = resVal;
        }
    })
});

